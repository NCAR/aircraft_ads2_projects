      set d2 = $DFILE:t
      echo "#   $d1/$d2 -- OK -- `date`"
    else
      echo "  msrcp job FAILED.  See email message for details."
    endif
  else
     echo " Cannot access file $SDIR/$SFILE"
  endif
  @ num ++
