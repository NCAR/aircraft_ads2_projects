#! /bin/csh -f
#
#  Archive processed MCR data files for 1998-101 SHEBA
#   (Done one file at a time to keep from overloading the network)
#
#  This script must be run from a project's Production/archive directory.
#
#  File adapted from file $PROJ_DIR/181/Production/archive/arch_mcr.181
#    by Ron Ruth  20 August 2001
#
#   To find executable (non-comment) lines (from within `vi'), type:
#       /^[^#]
#
#  `vi' :map commands for annotating job completion:
#  :map g eea 
#  :map v AOK @ 
#
#  `vi' commands for generating destination file names
#   (Source lines are marked "e" through "f" and cursor is at point
#    of insertion for destination lines.)
#     :'e,'fco.
#     mh''jmg
#     :'g,'hs/^........
#     :'g,'hs/r/R
#     :'g,'hs/f/F
#     :'e,'fs/^/#
#     :'g,'hs
#
#  Check login (only "dmg" login allowed to run this script)
set user = `whoami`
if ($user != "dmg") then
  echo "${0}:  Only the 'dmg' login is allowed to run this script.  Quitting."
  exit
endif
#
#
#  Set initialization info
set pfile = ../proj.info
set YEAR = `grep FY $pfile | sed "s/FY=//"`
set PROJ = `pwd | sed -e "s/\(.*\)\([0-9][0-9][0-9]\)\(.*\)/\2/g"`
#set SDIR = $TEMP
#set SDIR = $DATA_DIR
set SDIR = $DATA_DIR/mcr/SHEBA
set TURBRT = MCR
set logfile = mcr_arch_log.$PROJ
#
#  Set machine on which "msrcp" is to run (should be $MSRCP_MACH)
set msrcpMachine = "lenny"
#
#  Set read password if Restricted Access has been granted
set RPWD = ""
