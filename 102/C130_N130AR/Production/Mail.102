APCAS_IBL:long_name = "PCAS Raw Accumulation (per cell)" ;
APCAS_IBL:CellSizes = 0.1f, 0.12f, 0.14f, 0.17f, 0.2f, 0.25f, 0.3f, 0.4f, 0.5f, 0.7f, 0.9f, 1.2f, 1.5f, 2.f, 2.5f, 3.f ;
