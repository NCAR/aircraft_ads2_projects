#! /bin/csh -f
#
#  Archive NIMBUS netCDF output files for 4-201 Hong Kong (Kok)
#
set YEAR = 1994
set PROJ = 201
set TURB = LRT
#set TURB = HRT
#
#  Following are production output file names (sans extension) on $PROD_DATA.
set FILES =   ( 201RF67 201RF68 201RF69 201RF70 201RF71 201RF72 201RF73 \
                201RF74 201RF75 201RF76 201RF77 201RF78 201RF79 201RF80 \
                201RF81 201RF82 201RF83 201RF84 201RF85 201RF86 201RF87 \
                201RF88 201RF89 201RF90 201RF91 201RF92 201RF93 201RF94 \
                201RF95 201RF96 201RF97 )
set FLIGHTS = ( RF67    RF68    RF69    RF70    RF71    RF72    RF73 \
                RF74    RF75    RF76    RF77    RF78    RF79    RF80 \
                RF81    RF82    RF83    RF84    RF85    RF86    RF87 \
                RF88    RF89    RF90    RF91    RF92    RF93    RF94 \
                RF95    RF96    RF97 )
#
#  Get started:
date
set todo = $#FILES
set num = 1
while ($num <= $todo)
  echo \
  nrnet msput ${PROD_DATA}/${FILES[$num]}.cdf l df=tr \
    r flnm=/RAF/$YEAR/$PROJ/$TURB/${FLIGHTS[$num]}.cdf \
    rtpd=4095 w=RAFDMG mvn=CTRAFDMG
  @ num ++
end
