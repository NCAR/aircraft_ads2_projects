#
#
#  Check login (only "dmg" login allowed to run this script)
set user = `whoami`
if ($user != "dmg") then
  echo "${0}:  Only the 'dmg' login is allowed to run this script."
  echo "  Quitting"
  exit
endif
#
#  Initialize
#   Temporary file names
set Flist = "$TMPDIR/archcam_files.$$"
set Tfile = "$TMPDIR/tarit.archcam.$$"
set Afile = "$TMPDIR/archcam_$$.tar"
#   Tar file no-purge flag
set nopurge = 0
#   Verbose flag (no)
set vb = 0
#   Debug flag (no)
set db = 0
#   Production flag (no)
set pd = 0                #  Default is to echo, not archive the tar files
#
#  Get command-line arguments (if any)
#echo "  $#argv arguments:  $argv"
foreach arg ($argv)
#  echo "  Argument is: $arg"
  set ar = `echo $arg | sed -e "s/\-//"`
#   Verbose?
  if ($ar == "v") then
    set vb = 1
    continue
  endif
#   Debug?
  if ($ar == "d") then
    set db = 1
    continue
  endif
#   Production?
  if ($ar == "p") then
    set pd = 1
    continue
  endif
end
#
#  Get setup information for this project
set PROJ = `pwd | sed -e "s/\(.*\)\([0-9][0-9][0-9]\)\(.*\)/\2/g"`
#set PROJ = `pwd`
#set PROJ = `echo $PROJ | sed -e "s/\(.*\)\([0-9][0-9][0-9]\)\(.*\)/\2/g"`
set result = $status
if ($result != 0) then
  echo " Cannot find project number in this directory path"
  echo "  Quitting"
  exit 1
endif
#   Project directory
set Pdir = "$LOCAL/proj/$PROJ/Production"
#   archive directory
set Adir = "$Pdir/archive"
#   Project's information file
set pfile = $Pdir/proj.info
if (! -r $pfile) then
  echo "  Cannot access project's $pfile"
  echo "  Quitting"
  exit 1
endif
#   Get project information
#    full project name
set PN = (`grep "Information for Project" $pfile`)
if ($status == 0 && $#PN > 4) then
  shift PN
  shift PN
  shift PN
  shift PN
else
  set PN = "Unknown"
  echo "  Project Name not found in $pfile -- edit directory file(s) to insert"
  @ presult = 1
endif
#    fiscal year for directory path
##########  Change this to calendar year if one uses /ATD/DATA path
#            (See file /jnet/shared/proj/archives/scripts/gentl for example.)
set YEAR = `grep FY $pfile | sed -e "s/FY=//"`
set TYPE = "CAMERA"
#
#  Set machine on which "msrcp" is to run
if ($?MSRCP_MACH) then
  set msrcpMachine = $MSRCP_MACH
else
  set msrcpMachine = "vivaldi"
#  set msrcpMachine = "mahler"
#  set msrcpMachine = "lenny"
#  set msrcpMachine = "mistral"
endif
#
if ($pd != 1) then
  echo " ${0}:  This is a checkout run, since the -p option not set."
  echo "  No files will be archived to the Mass Store."
else
  echo " ${0}:  This is a production run."
  echo "  Tar files will be archived to the Mass Store."
endif
#
#
#  Get started:
if ($?SDIR && $?Sdir) then
  ls $SDIR/$Sdir >! $Flist
  set result = $status
  if ($result != 0) then
    echo "  Error $result when trying to get source file list from"
    echo "   directory $SDIR/$Sdir"
    goto quit
  endif
else
  echo "  Error:  source file directory not specified."
  goto quit
endif
#
#  Get/set indices
@ nfiles = `wc -l $Flist | awk '{print $1}'`
@ fztotal = 0
@ N = 0
@ n = 0
@ f = 0
set new = "yes"
#
if ($vb == 1) then
  echo ""
  echo "  Number of files in directory $Sdir = $nfiles"
endif
#
pushd $SDIR/$Sdir >& /dev/null
foreach file (`cat $Flist`)
  @ N ++
  @ n ++
#
##########  Add check here in case we are continuing a partial save
#
  if ($new == "yes") then
#   If new start, write boiler-plate for a tar command file
    echo "#! /bin/csh -f" >! $Tfile
    set result = $status
    if ($result != 0) then
      echo " Could not write temporary tar script file"
      goto quit
    endif
    echo "#" >> $Tfile
    echo "#  Prepare tar file for MSS archive" >> $Tfile
    echo "#  `date`" >> $Tfile
    echo "set OFILE = $Afile" >> $Tfile
    echo "#" >> $Tfile
    echo "cd $SDIR/$Sdir" >> $Tfile
    echo 'tar chvf $OFILE \' >> $Tfile
#   Save initial file name (with date/time info)
    set Sfile = $file
    set new = "no"
  endif
#  Get file's size
  @ fsz = `/bin/ls -l $file | awk '{print $5}'`
  @ fztotal = $fztotal + $fsz
#    Debug start
  if ($db == 1) then
    echo " File $file size = $fsz -- Total size = $fztotal"
  endif
#    Debug end
#  Check for a "full" tar file (size or # of files) or the end of archive list)
  if ( ($fztotal < $MAXSIZ) && ($n < $MAXTAR) && ($N != $nfiles) ) then
    echo "$file \" >> $Tfile
  else
#  Must now archive the files
    echo "$file" >> $Tfile
    set Efile = $file
#    Debug start
    if ($vb == 1) then
      echo "  First file in archive:  $Sfile"
      echo "  Last  file in archive:  $Efile"
      echo "  $n files to archive -- Total size = $fztotal"
    endif
#    Debug end
    chmod +x $Tfile
    echo "  Creating tar file $Afile"
#    Debug start
    if ($db == 1) then
      echo "  Pause to view tar script file:  $Tfile"
      echo -n "   hit [r] to continue  "
      set input = "$<"
    endif
#    Debug end
#  Create tar file (using script just generated)
    $Tfile >& /dev/null
    set result = $status
    if ($result != 0) then
      echo "  Error $result when trying to create tar file"
      goto quit
    endif
#
#  Get start/end times for archive (based on file's current name format)
    set Smeta = `echo $Sfile | sed -e "s/.jpg//"`
    set Smeta = `(echo $Smeta | awk -F_ '{print $1, $2, $3, $4}')`
    set Sdate = `echo $Smeta[3] | sed -e "s:\(..\)\(..\)\(..\):\2/\3/20\1:"`
    set Stime = `echo $Smeta[4] | sed -e "s/\(..\)\(..\)\(..\)/\1:\2:\3/"`
    set Emeta = `echo $Efile | sed -e "s/.jpg//"`
    set Emeta = `(echo $Emeta | awk -F_ '{print $1, $2, $3, $4}')`
    set Edate = `echo $Emeta[3] | sed -e "s:\(..\)\(..\)\(..\):\2/\3/20\1:"`
    set Etime = `echo $Emeta[4] | sed -e "s/\(..\)\(..\)\(..\)/\1:\2:\3/"`
#  Get camera location
    set Locn = `echo $Smeta[2] | tr '[a-z]' '[A-Z]'`
#  Compose archived tar file's MSS bitfile name (using metadata)
    set OFILE = `echo ${Locn}.20${Smeta[3]}.${Smeta[4]}_${Emeta[4]}.jpg.tar`
#  Create tar directory-list file (add header with metadata) and save it in
#   projects's archive subdirectory
#  Set file's full path name
    set dfile = "$Adir/$DDIR.$OFILE.dir"
    echo "  Creating tar directory file $Adir/$DDIR.$OFILE.dir"
    echo " Directory list for MSS bitfile /RAF/$YEAR/$PROJ/$TYPE/$DDIR/$OFILE" >! $dfile
    set result = $status
    if ($result != 0) then
      echo "  Error $result when trying to create tar file's directory list"
      echo "  Tar file $Afile will be saved"
      echo "   If this archive needs to be redone, start with file:  $Sdir/$Sfile"
      echo "   If continuing on, then use the next file after:  $Sdir/$Efile"
      set nopurge = 1
      goto quit
    endif
    echo "  C-130Q digital photos (jpg format) from project $PN" >> $dfile
    echo " " >> $dfile
    echo "  $TYPE $DDIR $Locn starts:  $Sdate $Stime" >> $dfile
    echo "  $TYPE $DDIR $Locn ends:  $Edate $Etime" >> $dfile
    echo "  Archive created on `date`" >> $dfile
    echo " " >> $dfile
    tar tvf $Afile >> $dfile
#
#  Write tar file to MSS if in production mode ($pd == 1)
    if (-r $Afile) then
      if ($pd == 1) then
        echo "  Archiving tar file $DDIR/$OFILE to MSS"
        echo "#  Job submitted on `date`"
        rsh $msrcpMachine msput_job -pe 32767 -pr 41113009 -wpwd RAFDMG $RPWD \
         $Afile mss:/RAF/$YEAR/$PROJ/$TYPE/$DDIR/$OFILE
        set result = $status
      else
        echo \
        rsh $msrcpMachine msput_job -pe 32767 -pr 41113009 -wpwd RAFDMG $RPWD \
         $Afile mss:/RAF/$YEAR/$PROJ/$TYPE/$DDIR/$OFILE
        set result = $status
      endif
      if ($result == 0) then
        echo "#  msrcp job for $DDIR/$OFILE -- OK -- `date`"
      else
        echo "#  msrcp job for $DDIR/$OFILE -- Failed -- `date`"
        echo "#                $DDIR/$OFILE:  error code $result"
      endif
    else
      echo "#  msrcp job for $DDIR/$OFILE -- Failed -- `date`"
      echo "#   Cannot access tar file $Afile"
    endif
#  Write tar directory-list file to MSS if in production mode ($pd == 1)
    if ($pd == 1) then
      echo "  Archiving tar directory file $OFILE.dir to MSS"
      echo "#  Job submitted on `date`"
      rsh $msrcpMachine msput_job -pe 32767 -pr 41113009 -wpwd RAFDMG $RPWD \
       $dfile mss:/RAF/$YEAR/$PROJ/$TYPE/$DDIR/$OFILE.dir
      set result = $status
    else
      echo \
      rsh $msrcpMachine msput_job -pe 32767 -pr 41113009 -wpwd RAFDMG $RPWD \
       $dfile mss:/RAF/$YEAR/$PROJ/$TYPE/$DDIR/$OFILE.dir
      set result = $status
    endif
    if ($result == 0) then
        echo "#  msrcp job for $DDIR/$OFILE.dir -- OK -- `date`"
    else
        echo "#  msrcp job for $DDIR/$OFILE.dir -- Failed -- `date`"
        echo "#                $DDIR/$OFILE.dir:  error code $result"
    endif
#    Debug start
    if ($pd != 1) then
      echo "  Number of files archived so far = $N"
      echo "  Number of files in this archive = $n"
      echo "  Total number of files to archive = $nfiles"
      echo "  Since this is a checkout run, pause to view files:"
      echo "   list of files to archive:  $Flist"
      echo "   tar script file:  $Tfile"
      echo "   tar file:  $Afile"
      echo "   tar directory file:  $dfile"
      echo -n "   hit [r] to continue  "
      set input = "$<"
    endif
#    Debug end
#  Reset for next wave
    \rm $Tfile
    \rm $Afile
    @ f ++
    @ n = 0
    @ fztotal = 0
    set new = "yes"
  endif
end
#
#  Done.
echo "#   Completed on `date`"
if ($f == 1) then
  echo -n "#   $f tar file"
else
  echo -n "#   $f tar files"
endif
if ($pd != 1) then
  echo -n " NOT"
endif
echo " archived to MSS"
goto cleanup
#
quit:
echo "  Quitting"
abort:
onintr -
#  Delete tar directory file if abnormal exit
if ($?dfile) then
  if (-e $dfile) then
    \rm $dfile
  endif
endif
cleanup:
if (-e $Flist) then
  \rm $Flist
endif
if (-e $Tfile) then
  \rm $Tfile
endif
if (-e $Afile) then
  if ($nopurge != 1) then
    \rm $Afile
  endif
endif
exit
